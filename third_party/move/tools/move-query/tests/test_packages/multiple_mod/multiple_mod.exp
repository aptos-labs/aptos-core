// === Source: 0x42::admin::admin_increment ===
    public fun admin_increment(state: &mut State, amount: u64, _cap: &AdminCap) {
        let current = core::get_value(state);
        let new_value = math::add(current, amount);
        core::set_value(state, new_value);
    }

