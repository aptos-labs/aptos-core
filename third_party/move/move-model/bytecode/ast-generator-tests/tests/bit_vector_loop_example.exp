public fun shift_left(self: &mut BitVector, amount: u64) {
        if (amount >= self.length) {
            vector::for_each_mut(&mut self.bit_field, |elem| {
                *elem = false;
            });
        } else {
            let i = amount;

            while (i < self.length) {
                if (is_index_set(self, i)) set(self, i - amount)
                else unset(self, i - amount);
                i = i + 1;
            };

            i = self.length - amount;

            while (i < self.length) {
                unset(self, i);
                i = i + 1;
            };
        }
    }

