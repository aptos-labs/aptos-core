//# publish
module 0x42::Math
public fun sum_(l0: u64, l1: u64): u64
    local l2: u64
    local l3: u64
    local l4: u64
    copy_loc l0
    ld_u64 0
    eq
    br_false l0
    move_loc l1
    ret
l0: copy_loc l0
    ld_u64 1
    sub
    st_loc l2
    move_loc l1
    move_loc l0
    add
    st_loc l3
    move_loc l2
    move_loc l3
    call sum_
    st_loc l4
    move_loc l4
    ret

public fun sum(l0: u64): u64
    local l1: u64
    move_loc l0
    ld_u64 0
    call sum_
    st_loc l1
    move_loc l1
    ret



