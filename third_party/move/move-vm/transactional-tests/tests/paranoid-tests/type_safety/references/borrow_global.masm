//# publish
module 0x22::A
struct G has drop + key
  x: u64

public fun make_g(): G
    ld_u64 1
    pack G
    ret

public fun store(l0: &signer)
    move_loc l0
    ld_u64 10
    pack G
    move_to G
    ret

public fun borrow(l0: address): &G
    move_loc l0
    borrow_global G
    ret

public fun borrow_mut(l0: address): &mut u64
    move_loc l0
    mut_borrow_global G
    ret

public fun borrow_field(l0: address): &u64
    local l1: &G
    move_loc l0
    borrow_global G
    st_loc l1
    move_loc l1
    borrow_field G, x
    ret

public fun borrow_field_2(l0: address): &u64
    local l1: &mut G
    move_loc l0
    mut_borrow_global G
    st_loc l1
    move_loc l1
    freeze_ref
    borrow_field G, x
    ret

public fun borrow_field_mut(l0: address): &mut u64
    local l1: &mut G
    move_loc l0
    mut_borrow_global G
    st_loc l1
    move_loc l1
    mut_borrow_field G, x
    ret



//# run --signers 0x1
script
