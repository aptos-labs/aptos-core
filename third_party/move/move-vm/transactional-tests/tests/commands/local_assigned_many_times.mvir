//# run
main() {
    let x: u64;
    let y: u64;
    let x_ref: &u64;
    let y_ref: &u64;
label b0:
    x = 5;
    y = 2;

    x_ref = &x;
    y_ref = &y;
    _ = move(y_ref);
    _ = move(x_ref);

    x_ref = &x;
    y_ref = move(x_ref);

    jump_if (true) b2;
label b1:
    _ = move(y_ref);
    x_ref = &x;
    y_ref = &y;
    jump b3;
label b2:
    _ = move(y_ref);
    x_ref = &y;
    y_ref = &x;
    jump b3;
label b3:
    assert(*move(x_ref) == 2, 42);
    _ = move(y_ref);
    return;
}
