//# publish --print-bytecode
module 0x66::test_pack_return_valid

struct S
    x: u64
    y: u64

struct T<T0>
    a: T0

// Valid: pack$S returns S
#[pack] public fun pack$S(l0: u64, l1: u64): S
    move_loc l0
    move_loc l1
    pack S
    ret

// Valid: pack$T returns T<u64>
#[pack] public fun pack$T<T0>(l0: T0): T<T0>
    move_loc l0
    pack T<T0>
    ret
