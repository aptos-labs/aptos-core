//# publish
module 0x42::M
struct S
  f: u64

public fun set_f(l0: &mut S, l1: u64)
    move_loc l1
    move_loc l0
    mut_borrow_field S, f
    write_ref
    ret

public fun t(l0: &mut u64, l1: &mut S, l2: &mut vector<u8>)
    ld_u64 112
    move_loc l0
    write_ref
    ld_u64 224
    move_loc l1
    mut_borrow_field S, f
    write_ref
    ld_u8 0
    move_loc l2
    ld_u64 1
    vec_mut_borrow <u8>
    write_ref
    ret



//# run --args 0 1 b"xyz"
script
use 0x42::M
entry public fun main(l0: &mut u64, l1: &mut M::S, l2: &mut vector<u8>)
    ld_u64 112
    move_loc l0
    write_ref
    move_loc l1
    ld_u64 224
    call M::set_f
    ld_u8 0
    move_loc l2
    ld_u64 1
    vec_mut_borrow <u8>
    write_ref
    ret



//# run 0x42::M::t --args 0 1 b"xyz"
