============ initial bytecode ================

[variant baseline]
fun m::foo($t0: bool, $t1: u64): u64 {
     var $t2: u64
     var $t3: u64
     var $t4: u64
     var $t5: u64
     var $t6: u64
  0: $t3 := infer($t1)
  1: if ($t0) goto 2 else goto 6
  2: label L0
  3: $t4 := 0
  4: $t3 := infer($t4)
  5: goto 7
  6: label L1
  7: label L2
  8: $t5 := infer($t3)
  9: $t6 := 1
 10: $t2 := +($t5, $t6)
 11: return $t2
}

