        let i = 0;
        while (i < 0x1::vector::length<u8>(flips)) {
            if (*0x1::vector::borrow<u8>(flips, i) != 0u8) break;
            i = i + 1;
        };
    }
    inline fun loops_without_break(flips: &vector<u8>) {
        let i = 0;
        while (i < 0x1::vector::length<u8>(flips)) {
            if (*0x1::vector::borrow<u8>(flips, i) == 0u8) () else abort 3;
            i = i + 1;
