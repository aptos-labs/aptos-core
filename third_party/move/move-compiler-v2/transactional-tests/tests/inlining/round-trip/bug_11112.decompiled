//**** Cross-compiled for `move` syntax from `tests/inlining/bug_11112.move`

//# publish
module 0xcafe::vectors {
    fun test_for_each_mut() {
        let _v0 = vector[1, 2, 3];
        let _v1 = 2;
        let _v2 = &mut _v0;
        let _v3 = 0;
        loop {
            let _v4 = 0x1::vector::length<u64>(freeze(_v2));
            if (!(_v3 < _v4)) break;
            let _v5 = 0x1::vector::borrow_mut<u64>(_v2, _v3);
            *_v5 = _v1;
            _v1 = _v1 + 1;
            _v3 = _v3 + 1;
            continue
        };
        assert!(_v0 == vector[2, 3, 4], 0);
    }
}


