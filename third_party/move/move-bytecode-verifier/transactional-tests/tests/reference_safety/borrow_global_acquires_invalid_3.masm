//# publish
module 0x1::A
use 0x1::signer
struct T1 has key
  v: u64

struct T2 has key
  v: u64

fun get_v(l0: &mut T1): &mut u64
    move_loc l0
    mut_borrow_field T1, v
    ret

fun acquires_t1(l0: &signer) acquires T1
    local l1: u64
    move_loc l0
    call signer::address_of
    move_from T1
    unpack T1
    st_loc l1
    ret

fun acquires_t2(l0: &signer) acquires T2
    local l1: u64
    move_loc l0
    call signer::address_of
    move_from T2
    unpack T2
    st_loc l1
    ret

public fun test1(l0: &signer) acquires T1, T2
    local l1: &mut T1
    local l2: &mut u64
    copy_loc l0
    call signer::address_of
    mut_borrow_global T1
    st_loc l1
    move_loc l1
    call get_v
    st_loc l2
    copy_loc l0
    call acquires_t1
    move_loc l0
    call acquires_t2
    move_loc l2
    pop
    ret



