//# publish
module 0x1::A
use 0x1::signer
struct T1 has key
  v: u64

public fun test1(l0: &signer, l1: &mut T1) acquires T1
    local l2: &mut u64
    copy_loc l0
    move_loc l1
    call borrow_acquires_t1
    st_loc l2
    move_loc l0
    call acquires_t1
    move_loc l2
    pop
    ret

fun borrow_acquires_t1(l0: &signer, l1: &mut T1): &mut u64 acquires T1
    move_loc l0
    call signer::address_of
    mut_borrow_global T1
    pop
    move_loc l1
    mut_borrow_field T1, v
    ret

fun acquires_t1(l0: &signer) acquires T1
    local l1: u64
    move_loc l0
    call signer::address_of
    move_from T1
    unpack T1
    st_loc l1
    ret