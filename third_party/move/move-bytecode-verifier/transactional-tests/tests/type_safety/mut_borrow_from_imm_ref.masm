//# publish
module 0x42::Token
struct T
  value: u64

public fun new(l0: u64): T
    copy_loc l0
    pack T
    ret

public fun destroy(l0: T)
    local l1: u64
    move_loc l0
    unpack T
    st_loc l1
    ret

public fun bump_value(l0: &T)
    local l1: &mut u64
    local l2: u64
    move_loc l0
    mut_borrow_field T, value
    st_loc l1
    copy_loc l1
    read_ref
    ld_u64 1
    add
    st_loc l2
    copy_loc l2
    move_loc l1
    write_ref
    ret