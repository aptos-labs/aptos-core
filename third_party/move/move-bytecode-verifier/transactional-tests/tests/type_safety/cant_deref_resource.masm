//# publish
module 0x1::Token
use 0x1::signer
struct T has key
  v: u64

public fun new(l0: u64): T
    move_loc l0
    pack T
    ret

public fun value(l0: &T): u64
    local l1: &u64
    local l2: u64
    move_loc l0
    borrow_field T, v
    st_loc l1
    move_loc l1
    read_ref
    st_loc l2
    move_loc l2
    ret

public fun exists(l0: address): bool
    local l1: bool
    move_loc l0
    exists T
    st_loc l1
    move_loc l1
    ret

public fun publish(l0: &signer, l1: T)
    move_loc l0
    move_loc l1
    move_to T
    ret

public fun test(l0: &signer) acquires T
    local l1: address
    local l2: T
    local l3: &mut T
    local l4: T
    copy_loc l0
    call signer::address_of
    st_loc l1
    ld_u64 0
    call new
    st_loc l2
    copy_loc l0
    move_loc l2
    call publish
    move_loc l1
    mut_borrow_global T
    st_loc l3
    move_loc l3
    read_ref
    st_loc l4
    ret