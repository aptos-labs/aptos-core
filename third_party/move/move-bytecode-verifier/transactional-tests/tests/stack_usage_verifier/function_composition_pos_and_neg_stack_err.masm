//# publish
module 0x1::A
public fun parity(l0: u64): (u64, bool)
    copy_loc l0
    move_loc l0
    ld_u64 2
    mod
    ld_u64 0
    eq
    ret

public fun takes_bool(l0: bool): u64
    move_loc l0
    br_true l0
    ld_u64 0
    ret
l0: ld_u64 1
    ret

public fun eat_stack(l0: u64)
    ret



//# run
script
use 0x1::A
entry public fun main()
    local l0: u64
    local l1: u64
    local l2: u64
    ld_u64 100
    st_loc l0
    move_loc l0
    call A::parity
    call A::takes_bool
    st_loc l1
    move_loc l1
    ld_true
    eq
    not
    br_false l0
    ld_u64 42
    abort
l0: call A::eat_stack
    call A::eat_stack
    ret