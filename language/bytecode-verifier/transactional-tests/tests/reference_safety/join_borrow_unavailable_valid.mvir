//# publish --address 0x1
module M {
    struct S { f: u64 }

    t1(root: &mut Self.S, cond: bool) {
        let u: u64;
        let x: &u64;

        u = 0;

        if (move(cond)) {
            x = &u;
        } else {
            _ = move(u);
            x = &move(root).f;
        }
        // it is okay for the two branches to disagree on the state of u, even though it is
        // borrowed in one branch
        return;
    }
}
