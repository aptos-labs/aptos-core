//# publish
module 0x1.A {
    public parity(u: u64): u64 * bool {
    label b0:
        return copy(u), (move(u) % 2 == 0);
    }

    public takes_bool(b: bool): u64 {
    label b0:
        jump_if (move(b)) b2;
    label b1:
        return 0;
    label b2:
        return 1;
    }
}

