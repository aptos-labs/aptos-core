{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "jsx": "react",
    "baseUrl": "src",
    "paths": {
      "core/*": ["./core/*"],
      "pages/*": ["./pages/*"],
      "shared/*": ["./shared/*"],
    },
  },
  "exclude": [
    "node_modules",
  ],
  "include": [
    ".eslintrc.js",
    "config-overrides.js",
    "src"
  ]
}
