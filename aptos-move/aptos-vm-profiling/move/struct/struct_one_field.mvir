module 0x1.M {
    struct Foo { x: u64 }

    run() {
        let i: u64;
        let x: u64;
        let foo: Self.Foo;

    label entry:
        i = 0;
    label loop_start:
        jump_if_false (copy(i) < 1000) loop_end;

        foo = Foo { x: 100 };
        Foo { x } = move(foo);

        i = move(i) + 1;
        jump loop_start;
    label loop_end:
        return;
    }
}
