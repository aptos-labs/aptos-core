;;;;;;;;;;;;;;;;;;;;;;
; rawIsEq
;;;;;;;;;;;;;;;;;;;;;;
; Compares two elements of any kind
; Params:
;   rdi <= Pointer to element 1
;   rsi <= Pointer to element 2
; Returns
;   ax <= 1 if are equal 0, otherwise
; Modified Registers:
;   rax
;;;;;;;;;;;;;;;;;;;;;;
<%=name%>_rawIsEq:
<%   for (let i=0; i<n64; i++) { %>
        mov     rax, [rsi + <%= (i*8) %>]
        cmp     [rdi + <%= (i*8) %>], rax
        jne     rawIsEq_ret0
<%   } %>
rawIsEq_ret1:
        mov    rax, 1
        ret

rawIsEq_ret0:
        xor    rax, rax
        ret

;;;;;;;;;;;;;;;;;;;;;;
; rawIsZero
