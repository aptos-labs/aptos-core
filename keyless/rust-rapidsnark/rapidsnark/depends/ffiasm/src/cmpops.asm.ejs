; Modified Registers:
;   rax
;;;;;;;;;;;;;;;;;;;;;;
<%=name%>_rawIsZero:
<%   for (let i=0; i<n64; i++) { %>
        cmp     qword [rdi + <%= (i*8) %>], $0
        jne     rawIsZero_ret0
<%   } %>

rawIsZero_ret1:
        mov    rax, 1
        ret

rawIsZero_ret0:
        xor    rax, rax
        ret
